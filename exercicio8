// 8. Escreva um progama em C no qual, dados dois vetores do mesmo tamanho,
// concatenar-os em um terceiro vetor.


#include <stdio.h>

void lerVetor (int n, int v[]) {

    for (int i = 0; i < n; i++){
        scanf ("%d", &v[i]);
    }
}

void imprimir (int tam, int v[]){
    for (int i = 0; i < tam; i++){
        printf ("%d", v[i]);
    }
}

int main () {
    int n, i;

    scanf ("%d", &n); 
    int v1[n], v2[n];

    //concatenacao dos vetores v1 e v2
    int tam = n + n;
    int v3[tam];

    lerVetor (n, v1);
    lerVetor (n, v2);

    for (int i = 0; i < n; i++) {
        v3[i] = v1[i];
    }

    int aux = 0;
    for (int i = n; i < tam; i++){
        v3[i] = v2[aux];
        aux++;
    }

    imprimir (tam, v3);
    return 0;
}
