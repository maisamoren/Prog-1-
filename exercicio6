// 6. Escreva um programa em C no qual, dados dois vetores de mesmo tamanho,
// multiplique cada posição de cada vetor e guarde o produto em um terceiro vetor.


#include <stdio.h>

int main () {

    int n, i;

    scanf ("%d", &n);

    int v1[n], v2[n], v3[n]; 

    if (n <= 1024) {
        for (int i = 0; i < n; i++){
            scanf ("%d", &v1[i]);
        }

        for (int i = 0; i < n; i++){
            scanf ("%d", &v2[i]);
        }
    }

    for (int i = 0; i < n; i++){
        v3[i] = v1[i] * v2[i];
        printf ("%d\n", v3[i]);
    }

    return 0;
}
