// HEADER
#include <stdio.h>
#define MAX = 100

void VetorVazio (int *v) {
	if (*v == -1) {
		return 1;

	return 0;
}

void Tamanho (int a){
	printf ("o tamanho do conjunto é: %d\n", a);
}

void InsertOrder (){
	int n, aux, j;
	int vetor;
	printf ("Digite um tamanho para o conjunto: %d\n", n);

	for (int i = 0; i < n; i++) {
		scanf ("%d", &vetor[i]);

		aux = vetor[i];
		j = i - 1;
		while (j>= 0) && (vetor[j] > aux){
			vetor[j+1] = vetor[j];
			j--;
		}
		vetor[j+1] = aux;
	}

}



// MAIN 
#include <stdio.h>

int main (){
	int menu;

	printf ("MENU: \n");
	printf ("\n ! OBSERVAÇÃO: o conjunto deve ser ed elementos inteiros positivos. \n");
	printf ("Digite 1 para verificar se um conjunto eh vazio. \n");
	printf ("Digite 2 para devolver o tamanho de um conjunto. \n");
	printf ("Digite 3 para preencher um conjunto. \n");
	printf ("Digite 4 para criar um conjunto de elementos aleatorios. \n");
	printf ("Digite 5 para ver um conjunto UNIAO de dois outros conjuntos. \n");
	printf ("Digite 6 para ver um conjunto INTERSECCAO de dois outros conjuntos. \n");

	scanf ("%d", &menu);

	return 0;
}
